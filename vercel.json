{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/flights/1/passengers",
            "dest": "./routes/flight.js"
        },
        {
            "src": "/(.*)",
            "dest": "./index.js"
        }
    ]
}